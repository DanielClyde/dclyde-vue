    "jsdom": "^22.1.0",
    "postcss": "^8.4.25",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "tailwindcss": "^3.2.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.2",
